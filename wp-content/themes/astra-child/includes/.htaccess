# Disable directory listing
Options -Indexes

# Block direct web access to PHP/PHAR files in this folder (includes still work)
<FilesMatch "\.(php|phar)$">
  <IfModule mod_authz_core.c>
    Require all denied
  </IfModule>
  <IfModule !mod_authz_core.c>
    Order deny,allow
    Deny from all
  </IfModule>
</FilesMatch>
